   <section id="main" type="standard">
		<!-- wrongly stemmed identity -->
		<e lm="baby"><i>baby</i><par n="bab/y__n"/></e>
		
		<!-- correctly stemmed identity -->
		<e lm="lady"><i>lad</i><par n="bab/y__n"/></e>
		
